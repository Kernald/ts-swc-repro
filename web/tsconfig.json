  "compilerOptions": {
    "lib": ["ES2015", "DOM"],
    "allowSyntheticDefaultImports": true,
    "declaration": true,
    "esModuleInterop": true,
    "noImplicitAny": true,
    "module": "CommonJS",
    "target": "es5",
    "jsx": "react",
    "moduleResolution": "node",
    "strict": true,
    "strictNullChecks": true
  }
}
